{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true,
      "minLength": 4,
      "maxLength": 150
    },
    "content": {
      "type": "richtext",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "SEO": {
      "displayName": "seo",
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    }
  }
}
